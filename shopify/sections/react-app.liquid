<section class="react-app-section">
  <div
    id="root"
    data-primary="{{ settings.primary_hsl }}"
    data-primary-foreground="{{ settings.primary_foreground_hsl }}"
    data-accent="{{ settings.accent_hsl }}"
    data-accent-foreground="{{ settings.accent_foreground_hsl }}"
    data-background="{{ settings.background_hsl }}"
    data-foreground="{{ settings.foreground_hsl }}"
    data-font-heading="{{ settings.font_heading }}"
    data-font-body="{{ settings.font_body }}"
  ></div>
</section>

{% schema %}
{
  "name": "React App",
  "settings": []
}
{% endschema %}
